
<% if not @ticket.errors.any?%>

$(".project_page_content").html("<%= escape_javascript(render :partial => 'tickets/index', :locals => { :project => @project } ) %>").hide().fadeIn();
proj_change_selected(1);

$(".project_page_content tr:last").focus();
<% else %>

var emsg = "";

<% @ticket.errors.full_messages.each do |msg| %>

  emsg += "<%= msg %><br/>" ;  

<% end %>

error_dialog(emsg, "Create Ticket Error");

<% end %>



