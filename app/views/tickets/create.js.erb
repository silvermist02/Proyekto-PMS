
<% if not @ticket.errors.any?%>

render_ajax("<%= @project.name %>","<%= escape_javascript(render 'tickets/index')%>");
nav_change_selected(1);
datepicker();
$(".page_content tr:last").focus();
<% else %>

var emsg = "";

<% @ticket.errors.full_messages.each do |msg| %>

  emsg += "<%= msg %><br/>" ;  

<% end %>

error_dialog(emsg, "Create Ticket Error");

<% end %>



